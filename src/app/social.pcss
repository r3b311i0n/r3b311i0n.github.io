.social-root > div:nth-child(1) {
    display: inline-flex;
    flex-direction: row;
    justify-content: center;
    width: 100%;
}

.social-root svg {
    height: 25px;
    width: 25px;
}

.social-motion-block {
    margin: auto 0.5rem 0.75rem 0.5rem;
}

.social-motion-block > a:any-link {
    cursor: pointer;
    outline: none;
}
